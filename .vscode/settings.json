{
	"deno.enablePaths": [
		"./deno.*",
		"./api",
		"./lib",
		"./scheduler",
		"./int-tests",
		"./slonik-pg-driver"
	],
	"editor.inlayHints.enabled": "off",
	"editor.tabSize": 2,
	"editor.formatOnSave": true,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js, ${capture}_test.ts"
	},
	"cSpell.words": [
		"logtape",
		"sffvektor"
	]
}
